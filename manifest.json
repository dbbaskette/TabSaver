{
  "manifest_version": 3,
  "name": "TabSaver",
  "version": "2.0",
  "description": "Manage and save your open Chrome tabs to organized bookmark folders.",
  "author": "dbbaskette",
  "permissions": [
    "tabs",
    "storage",
    "bookmarks"
  ],
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}