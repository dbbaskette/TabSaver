{
  "manifest_version": 3,
  "name": "nTabs",
  "version": "1.0",
  "description": "Manage and summarize your open tabs",
  "permissions": [
    "tabs",
    "storage",
    "scripting",
    "windows"
  ],
  "host_permissions": [
    "<all_urls>",
    "https://generativelanguage.googleapis.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
} 