{
  "name": "tabsaver",
  "version": "2.0.0",
  "description": "A modern Chrome extension to save tabs to organized bookmark folders with Apple Tahoe Liquid Glass UI",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "wxt build",
    "postinstall": "wxt prepare"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^6.2.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/chrome": "^0.1.12",
    "@types/node": "^24.5.2",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.39",
    "svelte": "^5.39.2",
    "svelte-check": "^4.3.1",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.5.3",
    "vite": "^7.1.6",
    "wxt": "^0.20.11"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.18",
    "lucide-svelte": "^0.544.0"
  }
}
