{
  "name": "tabsaver",
  "version": "2.0.0",
  "description": "A modern Chrome extension to save tabs to organized bookmark folders with Apple Tahoe Liquid Glass UI",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "wxt build",
    "postinstall": "wxt prepare"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/chrome": "^0.0.270",
    "@types/node": "^20.14.9",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.39",
    "svelte": "^4.2.18",
    "svelte-check": "^3.8.4",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.3",
    "vite": "^5.0.0",
    "wxt": "^0.19.0"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.13",
    "lucide-svelte": "^0.400.0"
  }
}